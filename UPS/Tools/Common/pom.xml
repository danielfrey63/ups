  <reporting>
    <plugins>
      <plugin>
        <artifactId>maven-changes-plugin</artifactId>
        <configuration>
          <issueLinkTemplate>%URL%/jira/ViewIssue.jspa?key=%ISSUE%</issueLinkTemplate>
          <xmlPath>${basedir}/xdocs/changes.xml</xmlPath>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-checkstyle-plugin</artifactId>
        <configuration>
          <configLocation>${basedir}/../UPS/conf/checkstyle.xml</configLocation>
          <headerLocation>${basedir}/../UPS/conf/checkstyle-header.txt</headerLocation>
        </configuration>
      </plugin>
    </plugins>
  </reporting>
  <distributionManagement>
    <site>
      <id>default</id>
