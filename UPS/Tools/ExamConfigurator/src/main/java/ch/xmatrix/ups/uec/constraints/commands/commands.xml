<?xml version="1.0" encoding="UTF-8"?>
<!--<!DOCTYPE commands SYSTEM "commands.dtd">-->
<commands version="1.1.0">
    <group id="toolbar">
        <face>
            <text>Toolbar</text>
        </face>
        <members>
            <member command-id="new"/>
            <member command-id="delete"/>
            <separator/>
            <member command-id="add"/>
            <member command-id="remove"/>
        </members>
    </group>
    <command id="new">
        <face>
            <text i18n="action.new.text"/>
            <icon type="icon-factory">createDropShadowIcon(cross_big1.png, #9000000)</icon>
            <selected-icon type="icon-factory">createPressedIcon(cross_big1.png, #900000)</selected-icon>
            <description i18n="action.new.description"/>
        </face>
        <face name="toolbar" extends="[default]">
            <text/>
        </face>
    </command>
    <command id="delete">
        <face>
            <text i18n="action.delete.text"/>
            <icon type="icon-factory">createDropShadowIcon(bars_big1.png, #9000000)</icon>
            <selected-icon type="icon-factory">createPressedIcon(bars_big1.png, #900000)</selected-icon>
            <description i18n="action.delete.description"/>
        </face>
        <face name="toolbar" extends="[default]">
            <text/>
        </face>
    </command>
    <command id="add">
        <face>
            <text i18n="action.add.text"/>
            <icon type="icon-factory">createDropShadowIcon(arrow_left.png, #9000000)</icon>
            <selected-icon type="icon-factory">createPressedIcon(arrow_left.png, #900000)</selected-icon>
            <description i18n="action.add.description"/>
        </face>
        <face name="toolbar" extends="[default]">
            <text/>
        </face>
    </command>
    <command id="remove">
        <face>
            <text i18n="action.remove.text"/>
            <icon type="icon-factory">createDropShadowIcon(arrow_right.png, #9000000)</icon>
            <selected-icon type="icon-factory">createPressedIcon(arrow_right.png, #900000)</selected-icon>
            <description i18n="action.remove.description"/>
        </face>
        <face name="toolbar" extends="[default]">
            <text/>
        </face>
    </command>
</commands>
