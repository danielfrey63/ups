<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.wegmueller.ups.storage.beans">

    <class name="PruefungsListe" table="pruefungsliste">
        <id name="id" type="long">
            <generator class="identity"/>
        </id>
        <property name="pruefungsSession" type="string"/>
        <property name="pruefung" type="string"/>
        <property name="userName" type="string"/>
        <property name="studentenNummer" type="string"/>
        <property name="pruefungsListe" type="com.wegmueller.ups.storage.hibernate.ByteType"/>
        <property name="pdf" type="com.wegmueller.ups.storage.hibernate.ByteType"/>
        <property name="properties" type="com.wegmueller.ups.storage.hibernate.ByteType"/>
    </class>
    <class name="OIS2LDAP" table="ois2ldap">
        <id name="ldap" type="string">
            <generator class="assigned"/>
        </id>
        <property name="oisusername" type="string"/>
        <property name="oispassword" type="string"/>
    </class>
    <class name="PruefungsSession" table="pruefungssession">
        <id name="seskez" type="string">
            <generator class="assigned"/>
        </id>
        <property name="sessionsname" type="string"/>
        <property name="sessionsende" type="calendar"/>
        <property name="planungFreigabe" type="calendar"/>
        <property name="storageDate" type="calendar"/>
    </class>

    <class name="Anmeldedaten" table="anmeldedaten">
        <id name="id" type="long">
            <generator class="identity"/>
        </id>

        <property name="dozentUserName" type="string"/>

        <property name="pruefungsmodeText" type="string"/>
        <property name="fachrichtung" type="string"/>
        <property name="studentennummer" type="string"/>
        <property name="vorname" type="string"/>
        <property name="repetent" type="boolean"/>
        <property name="pruefungsraum" type="string"/>
        <property name="nachname" type="string"/>
        <property name="studiengang" type="string"/>
        <property name="email" type="string"/>
        <property name="lkNummer" type="string"/>
        <property name="lkForm" type="integer"/>
        <property name="lkFormText" type="string"/>
        <property name="lkEinheitTitel" type="string"/>
        <property name="lkEinheitTyp" type="string"/>
        <property name="lkEinheitNummerzusatz" type="string"/>
        <property name="lkEinheitTypText" type="string"/>
        <property name="seskez" type="string"/>
        <property name="pruefungsdatum" type="calendar"/>
        <property name="pruefungsdatumVon" type="calendar"/>
        <property name="pruefungsdatumBis" type="calendar"/>
    </class>

</hibernate-mapping>