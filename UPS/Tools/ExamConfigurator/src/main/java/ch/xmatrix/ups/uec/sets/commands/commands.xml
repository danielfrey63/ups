<?xml version="1.0" encoding="UTF-8"?>
<!--<!DOCTYPE commands SYSTEM "commands.dtd">-->
<commands version="1.1.0">
    <group id="toolbar">
        <face>
            <text>Toolbar</text>
        </face>
        <members>
            <member command-id="refresh"/>
            <separator/>
            <member command-id="fromFiles"/>
            <member command-id="fromDirectory"/>
            <member command-id="fromInternet"/>
            <member command-id="newPerson"/>
            <separator/>
            <member command-id="reloadServerData"/>
            <separator/>
            <member command-id="remove"/>
            <separator/>
            <member command-id="moveUp"/>
            <member command-id="moveDown"/>
            <separator/>
            <member command-id="calculate"/>
