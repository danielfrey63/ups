<?xml version="1.0" encoding="UTF-8"?>
<!--<!DOCTYPE commands SYSTEM "commands.dtd">-->
<commands version="1.1.0">
    <group id="toolbar">
        <face>
            <text>Toolbar</text>
        </face>
        <members>
            <member command-id="master.add"/>
            <member command-id="master.copy"/>
            <member command-id="master.delete"/>
            <separator/>
            <member command-id="master.fix"/>
            <separator/>
            <member command-id="master.load"/>
            <member command-id="master.save"/>
        </members>
    </group>
    <command id="master.add">
        <face>
            <text i18n="action.add.text"/>
            <icon type="icon-factory">createDropShadowIcon(cross_big1.png, #00FF00)</icon>
            <selected-icon type="icon-factory">createPressedIcon(cross_big1.png, #00FF00)</selected-icon>
            <description i18n="action.add.description"/>
            <accelerator key="i18n:action.add.accelerator" useDefaultModifier="true"/>
        </face>
        <face name="label" extends="[default]">
            <text i18n="action.add.longdescription"/>
        </face>
        <face name="toolbar" extends="[default]">
            <text/>
        </face>
    </command>
    <command id="master.copy">
        <face>
            <text i18n="action.copy.text"/>
            <icon type="icon-factory">createDropShadowIcon(cross_big2.png, #00FF00)</icon>
            <selected-icon type="icon-factory">createPressedIcon(cross_big2.png, #00FF00)</selected-icon>
            <description i18n="action.copy.description"/>
            <accelerator key="i18n:action.copy.accelerator" useDefaultModifier="true"/>
        </face>
        <face name="label" extends="[default]">
            <text i18n="action.add.longdescription"/>
        </face>
        <face name="toolbar" extends="[default]">
            <text/>
        </face>
    </command>
    <command id="master.delete">
        <face>
            <text i18n="action.delete.text"/>
            <icon type="icon-factory">createDropShadowIcon(bars_big1.png, #00FF00)</icon>
            <selected-icon type="icon-factory">createPressedIcon(bars_big1.png, #00FF00)</selected-icon>
            <description i18n="action.delete.description"/>
            <accelerator key="i18n:action.delete.accelerator" useDefaultModifier="true"/>
        </face>
        <face name="label" extends="[default]">
            <text i18n="action.delete.longdescription"/>
        </face>
        <face name="toolbar" extends="[default]">
            <text/>
        </face>
    </command>
    <command id="master.load">
        <face>
            <text i18n="action.load.text"/>
            <icon type="icon-factory">createDropShadowIcon(floppy_open.png, #00FF00)</icon>
            <selected-icon type="icon-factory">createPressedIcon(floppy_open.png, #00FF00)</selected-icon>
            <description i18n="action.load.description"/>
            <accelerator keyStroke="i18n:action.load.accelerator"/>
        </face>
        <face name="label" extends="[default]">
            <text i18n="action.load.longdescription"/>
        </face>
        <face name="toolbar" extends="[default]">
            <text/>
        </face>
    </command>
    <command id="master.save">
        <face>
            <text i18n="action.save.text"/>
            <icon type="icon-factory">createDropShadowIcon(floppy_save.png, #00FF00)</icon>
            <selected-icon type="icon-factory">createPressedIcon(floppy_save.png, #00FF00)</selected-icon>
            <description i18n="action.save.description"/>
            <accelerator keyStroke="i18n:action.save.accelerator"/>
        </face>
        <face name="label" extends="[default]">
            <text i18n="action.save.longdescription"/>
        </face>
        <face name="toolbar" extends="[default]">
            <text/>
        </face>
    </command>
    <command id="master.fix">
        <face>
            <text i18n="action.fix.text"/>
            <icon type="icon-factory">createDropShadowIcon(checkbox_big1_off.png, #00FF00)</icon>
            <selected-icon type="icon-factory">createPressedIcon(checkbox_big1_off.png, #00FF00)</selected-icon>
            <description i18n="action.fix.description"/>
            <accelerator key="i18n:action.fix.accelerator" useDefaultModifier="true"/>
        </face>
        <face name="label" extends="[default]">
            <text i18n="action.save.longdescription"/>
        </face>
        <face name="toolbar" extends="[default]">
            <text/>
        </face>
    </command>
</commands>
