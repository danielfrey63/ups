    </group>

    <command id="refresh">
        <face>
            <text i18n="action.refresh.text"/>
            <icon type="icon-factory">getIcon(/com/famfamfam/silk/arrow_refresh.png)</icon>
            <selected-icon type="icon-factory">createPressedIcon(/com/famfamfam/silk/arrow_refresh.png)</selected-icon>
            <description i18n="action.refresh.description"/>
            <accelerator key="i18n:action.refresh.accelerator" useDefaultModifier="true"/>
        </face>
        <face name="toolbar" extends="[default]">
            <text/>
        </face>
    </command>

