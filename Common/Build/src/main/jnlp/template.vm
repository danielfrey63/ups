<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE jnlp PUBLIC "-//Sun Microsystems, Inc//DTD JNLP Discriptor 1.5//EN" "http://java.sun.com/dtd/JNLP-1.5.dtd">
<jnlp
        spec="$jnlpspec"
        codebase="$codebase"
        href="$outputFile">
    <information>
        <title>$project.Name</title>
        <vendor>$project.Organization.Name</vendor>
        <homepage href="$project.Url"/>
        <description>$project.Description</description>
#if($offlineAllowed)        <offline-allowed/>#end
        <shortcut online="false">
            <desktop/>
            <menu submenu="eBot 6"/>
        </shortcut>
    </information>
#if($allPermissions)    <security>
        <all-permissions/>
    </security>
    #end<resources>
        <j2se #if($max-heap-size)max-heap-size="$max-heap-size"#end version="1.6+"/>
        $dependencies
        <property name="file.encoding" value="windows-1252"/>
    </resources>
    <application-desc main-class="$mainClass"/>
</jnlp>
